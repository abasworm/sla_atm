.login-box
  .login-logo
    a(href='/')
      b=title
      
  // /.login-logo
  .login-box-body
    #alertbox.alert.alert-warning
      a#closeAlert.close.btn.btn-sm(href='javascript:;')
          i.fa.fa-times
      text#message
        
    p.login-box-msg Sign in to start your session
    form(action='javascript:;' method='post')
      .form-group.has-feedback
        input.form-control(type='text' placeholder='username' id='username', autocomplete='off')
        span.glyphicon.glyphicon-envelope.form-control-feedback
      .form-group.has-feedback
        input.form-control(type='password' placeholder='Password' id='password')
        span.glyphicon.glyphicon-lock.form-control-feedback
      .row
        .col-xs-8
        
        .col-xs-4
          a.btn.btn-primary.btn-block.btn-flat(id="btn_login") Sign In
        // /.col
    
    
  // /.login-box-body
